#!/bin/sh

set -e

# Using the `with-fee` without providing an amount should query the network for
# the configuration and use what is declared as the minimal fee.

bnscli send-tokens \
		-src "seq:test/bnscli/1" \
		-dst "seq:test/bnscli/2" \
		-amount "4 IOV" \
		-memo "bnscli test" \
	| bnscli with-fee \
	| bnscli view
